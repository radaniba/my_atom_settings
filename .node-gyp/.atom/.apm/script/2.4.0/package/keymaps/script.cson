# https://atom.io/docs/latest/advanced/keymaps

# Only bind the keys in the editor
'.platform-darwin .editor':
  'cmd-i': 'script:run'
  'ctrl-w': 'script:close-view'
  'ctrl-c': 'script:kill-process'
  'shift-cmd-i': 'script:run-options'
  'shift-enter': 'script:run'
